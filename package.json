{
  "name": "chainpay",
  "version": "0.1.0",
  "description": "Chainpay",
  "author": "Jawad Ahmed <contact@jahmed.me>",
  "main": "index.js",
  "private": false,
  "engines": {
    "node": ">=6.9.1",
    "npm": ">=2.15.11",
    "yarn": ">=0.20.3"
  },
  "scripts": {
    "start": "node ./dist/index.js",
    "start:build": "gulp serve",
    "queue": "node ./dist/queue/index.js",
    "queue:build": "yarn build && node ./dist/queue/index.js",
    "cron": "node ./dist/cronjob/index.js",
    "cron:build": "yarn build && node ./dist/cronjob/index.js",
    "initialize": "node ./dist/scripts/init.js",
    "initialize:build": "yarn build && node ./dist/scripts/init.js",
    "check": "node ./dist/scripts/check.js",
    "check:build": "yarn build && node ./dist/scripts/check.js",
    "config": "node ./dist/config/config.js",
    "crypto": "node ./dist/scripts/encrypt.js",
    "crypto:build": "yarn build && node ./dist/scripts/encrypt.js",
    "start:debug": "cross-env DEBUG=amida-api-boilerplate:* yarn start",
    "build": "gulp",
    "lint": "esw *.js server config --color",
    "lint:watch": "yarn lint -- --watch",
    "beautify": "es-beautifier -u -c .eslintrc.js *.js server config",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --ui bdd --reporter spec --slow 0 --colors --require babel-core/register tests --recursive --timeout 1500000",
    "test:watch": "yarn test -- --watch",
    "test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover _mocha -- --ui bdd --reporter spec --colors --compilers js:babel-core/register tests --recursive",
    "test:check-coverage": "yarn test:coverage && istanbul check-coverage",
    "report-coverage": "coveralls < ./coverage/lcov.info"
  },
  "dependencies": {
    "bluebird": "3.5.5",
    "body-parser": "1.19.0",
    "chai": "^4.2.0",
    "compression": "1.7.4",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "debug": "^4.1.1",
    "del": "^4.1.1",
    "dotenv": "^8.0.0",
    "ethereumjs-tx": "^1.3.7",
    "express": "^4.17.1",
    "express-validation": "1.0.2",
    "express-winston": "3.1.0",
    "gulp": "^4.0.2",
    "gulp-load-plugins": "^1.6.0",
    "helmet": "3.18.0",
    "http-status": "^1.3.2",
    "joi": "14.3.1",
    "lodash": "4.17.11",
    "method-override": "^3.0.0",
    "morgan": "1.9.1",
    "mysql": "2.17.1",
    "mysql2": "^1.6.5",
    "node-cron": "^2.0.3",
    "redis": "^2.8.0",
    "request": "^2.88.0",
    "run-sequence": "^2.2.1",
    "sequelize": "^5.8.7",
    "socket.io": "^2.2.0",
    "supertest": "4.0.2",
    "supertest-as-promised": "4.0.2",
    "uuid": "^3.3.2",
    "web3": "1.0.0-beta.34",
    "winston": "3.2.1",
    "winston-papertrail": "^1.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "babel-cli": "6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-add-module-exports": "1.0.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "commitizen": "^3.1.1",
    "coveralls": "^3.0.3",
    "cross-env": "5.2.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.17.3",
    "eslint-watch": "5.1.2",
    "gulp-babel": "^7.0.1",
    "gulp-newer": "^1.4.0",
    "gulp-nodemon": "^2.4.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-util": "^3.0.8",
    "husky": "^2.3.0",
    "istanbul": "1.1.0-alpha.1",
    "loadtest": "^3.0.7",
    "mocha": "^6.1.4",
    "validate-commit-msg": "^2.14.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "add-module-exports"
    ]
  }
}
